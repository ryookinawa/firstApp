<!DOCTYPE html>
<html lang="ja" ng-app>
  <head>
    <meta charset="utf-8">
    <title><%= title %></title>
  </head>
    <!--<script src="js/angular.js"></script>-
    <script src="js/angular-resource.js"></script>-->
    <!--<script src="app.js"></script>-->
    <script src="//code.angularjs.org/1.3.15/angular.min.js"></script>
    <script src="//code.angularjs.org/1.3.15/angular-resource.min.js"></script>
    <script src="//code.angularjs.org/1.3.15/angular-route.min.js"></script>
    <script src="./javascript/app.js"></script>

  <body>
      <%= title %>

    <hr noshade size="3">
    <input type="number" ng-model="num">
    <div>1 + {{num}} = {{1 + num}}</div>

    <div ng-controller="MainCtrl">
      <input type="text" ng-model="deviceId">
      <input type="text" ng-model="clientKey">
      <input type="text" ng-model="irCode">
      <pre>{{ irCode }}</pre>
    </div>

    <hr noshade size="3">
  </body>
</html>
